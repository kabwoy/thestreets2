<body id="contact-body" style="overflow-x: hidden;">
 
    <div>
        <%- include("./partials/navbar") %> 
        <div class="container mt-4" style="margin-top: 10% !important;">
        <form action="/images" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <input type="file" class="form-control-file" multiple name="image" >
            </div>

            <button type="submit" class="submit animated bounce delay-4s learn-btn">
                Add Image
             </button>
        </form>
    </div>
      
    <div class="menu-wrap">
        <input type="checkbox" class="toggler">
        <div class="hamburger">
            <div></div>
        </div>
        <div class="menu">
            <%- include('./partials/burger') %> 
        </div>
    </div>


